<h2>ログイン</h2>
<%= form_tag("/auth/login_auth" , method: "post") do %>
<table>
  <tr>
    <td><%= label_tag(:username, "ユーザー名：") %></td>
    <td><%= text_field_tag(:username) %></td>
  </tr>
  <tr>
    <td><%= label_tag(:password, "パスワード：") %></td>
    <td><%= text_field_tag(:password) %></td>
  </tr>
</table>
 <%= submit_tag("ログイン") %>
<% end %>
<a href="<%= auth_signup_path%>">アカウント未登録の方はこちら</a>
